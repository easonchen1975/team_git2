<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<BuildTarget tool="xtensa" version="1.0">
    <BuildSettings>
        <BaseSettings path="project">
            <CompilerOptions>
                <SingleFlagMapOptions>
                    <SingleFlagMapEntry>
                        <key>OptimizeSpace</key>
                        <value flag="-Os" use="true"/>
                    </SingleFlagMapEntry>
                </SingleFlagMapOptions>
                <FlagValueMapOptions>
                    <FlagValueMapEntry>
                        <key>Optimization</key>
                        <value level="2" flag="-O" use="true"/>
                    </FlagValueMapEntry>
                    <FlagValueMapEntry>
                        <key>Debug</key>
                        <value level="-3" flag="-g" use="true"/>
                    </FlagValueMapEntry>
                </FlagValueMapOptions>
            </CompilerOptions>
            <LinkerOptions>
                <BooleanMapOptions>
                    <BooleanMapEntry>
                        <key>GenerateMapFile</key>
                        <value selected="true"/>
                    </BooleanMapEntry>
                    <BooleanMapEntry>
                        <key>CreateMinsize</key>
                        <value selected="true"/>
                    </BooleanMapEntry>
                </BooleanMapOptions>
                <LinkerSupport custom="false" key="-mlsp=" value="sim-stacklocal"/>
            </LinkerOptions>
        </BaseSettings>
    </BuildSettings>
</BuildTarget>
